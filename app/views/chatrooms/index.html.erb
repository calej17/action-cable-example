<% if @current_user %>
  <h3>Hey, <%= @current_user.username %>. <%= link_to "Sign out?", signout_path, method: :delete %></h3>
<% else %>
  <%= link_to "Sign Up!", new_user_path %> | <%= link_to "Sign In!", signin_path %>
<% end %>

<br>
<h3>Chatrooms:</h3>
<ul>
  <% @chatrooms.each do |room| %>
    <li>
      <%= link_to "Chatroom #{room.id}", chatroom_path(room) %>
    </li>
  <% end %>
</ul>
